Module
  name: mapeditor
  description: Initialize the mapeditor and setups its main window
  author: fallen, edubart
  reloadable: false

  load-later:
    - mapeditor_skins
    - mapeditor_topmenu
    - mapeditor_terminal
    - mapeditor_modulemanager
    - mapeditor_interface
    - mapeditor_minimap

  @onLoad: |
    dofile 'mapeditor'
    MapEditor.init()

  @onUnload: |
    MapEditor.terminate()
